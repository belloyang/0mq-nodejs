{
  "name": "@nanometrics/pegasus-harvest-lib",
  "version": "1.3.5",
  "description": "Pegasus Harvester Library Package",
  "main": "index.js",
  "author": "Nanometrics Inc.",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:lib": "node-gyp rebuild",
    "build:cli": "cd ./HarvesterApp && node-gyp rebuild",
    "build:npm": "yarn clean && tsc",
    "postbuild:npm": "cp -rf HarvesterApp TelemetryLib HarvLib LibIO PDBLib PSFLib third-party binding.gyp package.json shared_defines.h version.h ./dist/ && cp -rf nodejs-api/src nodejs-api/*.h nodejs-api/*.cc ./dist/nodejs-api/",
    "publish:npm": "yarn build:npm && npm publish ./dist"
  },
  "publishConfig": {
    "registry": "https://nexus.nmx.ca/repository/npm-releases/"
  },
  "devDependencies": {
    "@types/node": "^12.12.17",
    "ffi": "^2.3.0",
    "gyp": "^0.5.0",
    "node-gyp": "^3.8.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  }
}
